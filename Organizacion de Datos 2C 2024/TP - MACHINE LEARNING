licks.\n","      quickchartButtonEl.classList.add('colab-df-spinner');\n","      try {\n","        const charts = await google.colab.kernel.invokeFunction(\n","            'suggestCharts', [key], {});\n","      } catch (error) {\n","        console.error('Error during call to suggestCharts:', error);\n","      }\n","      quickchartButtonEl.classList.remove('colab-df-spinner');\n","      quickchartButtonEl.classList.add('colab-df-quickchart-complete');\n","    }\n","    (() => {\n","      let quickchartButtonEl =\n","        document.querySelector('#df-314397f9-226d-48d1-9115-7177307a5a2f button');\n","      quickchartButtonEl.style.display =\n","        google.colab.kernel.accessAllowed ? 'block' : 'none';\n","    })();\n","  </script>\n","</div>\n","\n","    </div>\n","  </div>\n"],"application/vnd.google.colaboratory.intrinsic+json":{"type":"dataframe","variable_name":"transactions_train"}},"metadata":{},"execution_count":15}]},{"cell_type":"code","source":["pd.set_option('display.max_rows', None)  # Para mostrar todas las filas\n","pd.set_option('display.max_columns', None)  # Para mostrar todas las columnas\n","transactions_train.count()"],"metadata":{"id":"3jSGjvoywb6g","executionInfo":{"status":"ok","timestamp":1731461999671,"user_tz":180,"elapsed":293859,"user":{"displayName":"David Leonardo Batallan","userId":"16328448998948482899"}}},"execution_count":16,"outputs":[]},{"cell_type":"markdown","source":["# **DATOS identity_features_train.csv**"],"metadata":{"id":"MBOBzDuBXedB"}},{"cell_type":"code","source":["# identity_features_train.csv en https://drive.google.com/file/d/1u0sbL4T5wy6eag2aZclxexOxq9C3VblZ/view?usp=drive_link\n","\n","id='1u0sbL4T5wy6eag2aZclxexOxq9C3VblZ'\n","downloaded = drive.CreateFile({'id': id})\n","downloaded.GetContentFile('identity_features_train.csv')"],"metadata":{"id":"sOg43tkmYTxZ","executionInfo":{"status":"ok","timestamp":1731460811660,"user_tz":180,"elapsed":2515,"user":{"displayName":"David Leonardo Batallan","userId":"16328